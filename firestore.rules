rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read;
    }
  }
  match /databases/{database}/documents {
    match /{document=**} {
      allow read; allow write: if request.auth != null && request.auth.uid == "8gZKzIAI7le5B03GbynBUKCpyl02";
       allow write: if request.auth != null && request.auth.uid == "zj0jKGLWbUPb7FapAUoCS9zyaoo1";
      
    }
  }
  match /databases/1/documents {
    match /{document=**} {
      allow read, write;
    }
  }
}